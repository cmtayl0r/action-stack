{
  "sitemap": "http://localhost:5173/sitemap.xml",
  "defaults": {
    "timeout": 20000,
    "wait": 500,
    "chromeLaunchConfig": {
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox"
      ]
    },
    "rules": [
      "WCAG2AA"
    ],
    "ignore": [
      "notice",
      "warning"
    ]
  },
  "urls": [
    "http://localhost:5173",
    "http://localhost:5173/about",
    "http://localhost:5173/contact"
  ]
}
